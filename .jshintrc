{
  "curly": true,
  "eqnull": true,
  "eqeqeq": true,
  "undef": true,
  "unused": "vars",
  "laxcomma": true,
  "laxbreak": true,
  //"latedef": true,
  "sub": true,
  "bitwise": true,
  "forin": true,
  "immed": true,
  "newcap": true,
  "nocomma": true,
  "noempty": true,
  "nonew": true,
  "strict": true,
  "globals": {
    "angular": true,
    // for requirejs
    "define": false,
    "require": false,
    "console": true,
    // jasmine
    "spyOn": false,
    "spyOnEvent": false,
    "xit": false,
    "xdescribe": false,
    "performance": true
  },
  "maxcomplexity": 5,
  "maxdepth": 3,
  "browser": true,
  "jquery": true,
  "jasmine": true,
  // WARN: relaxing options:
  "asi": true,
  "funcscope": true
}
